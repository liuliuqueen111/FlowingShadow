{
  "name": "ssr-blog-ai-pro",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr entry-server.tsx --outDir dist/server",
    "start": "NODE_ENV=production node server.js",
    "start:win": "set NODE_ENV=production && node server.js",
    "preview": "vite preview",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-validator": "^7.3.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sequelize": "^6.37.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.14.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "typescript": "~5.8.2",
    "vite": "^5.4.8"
  }
}
